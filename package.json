{
  "name": "r-plumber-app",
  "version": "1.0.0",
  "description": "using r plumber for interactive web apps",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "Rscript -e \"source('server.R')\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
