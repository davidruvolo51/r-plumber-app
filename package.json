{
  "name": "r-plumber-app",
  "version": "1.0.0",
  "description": "using r plumber for interactive web apps",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn build",
    "server": "Rscript -e \"source('server.R')\"",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "keywords": [
    "r",
    "webapps",
    "plumber"
  ],
  "author": "dcruvolo",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.0.0"
  },
  "dependencies": {
    "parcel": "^2.9.3",
    "react": "^18.2.0"
  }
}
